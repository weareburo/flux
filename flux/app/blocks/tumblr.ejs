
<section data-provider="<%= items[i].source_service %>"  data-type="<%= items[i].type %>">
	
	<div <% if(items[i].picture.url){ %> style="background-image:url('<%= items[i].picture.url %>')" class="story-content"  <% }else{ %> class="story-content no-bg"  <% }%>>

		<div class="tint">
			
			<div class='icons'>
				<span class="entypo-social c-<%= items[i].source_service %>"></span> <%= items[i].dateFormat %> <%= items[i].popularity %> notes
			</div>

			<div class="hover">
				<div class='title'><h2><span class="entypo-social c-<%= items[i].source_service %>"></span> <%= items[i].title %></h2></div>
				<p><%= items[i].message %></p>
			</div>

		</div>			
	</div>

</section>
